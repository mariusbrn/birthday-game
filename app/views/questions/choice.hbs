
<div class="row">
	<div class="col s12 m12">
	  <div class="card blue-grey darken-1">
	    <div class="card-content white-text">
        <p><em>{{ question.sentence }}</em></p>
        <span class="card-title">{{ question.question }}</span>
        <div class="row">
        {{#each question.choices }}
          <a class="waves-effect waves-light btn-large btn-block btn-choice col s12 m12">{{.}}</a>
        {{/each}}
        </div>
	    </div>
	  </div>
	</div>
</div>